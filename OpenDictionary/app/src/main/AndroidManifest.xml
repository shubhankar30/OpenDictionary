<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="shubhankar30.opendictionary">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        tools:ignore="GoogleAppIndexingWarning"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="shubhankar30.opendictionary.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="shubhankar30.opendictionary.ListDataActivity"></activity>
        <activity android:name="shubhankar30.opendictionary.EditDataActivity"></activity>
    </application>

</manifest>